@model StajyerTakipSistemi.Web.Models.SFinal

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create</h1>

<h4>SFinal</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="InternId" class="control-label"></label>
                <select asp-for="InternId" class ="form-control" asp-items="ViewBag.InternId"></select>
            </div>
            <div class="form-group">
                <label asp-for="RelatedDocuments" class="control-label"></label>
                <input asp-for="RelatedDocuments" class="form-control" />
                <span asp-validation-for="RelatedDocuments" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GitHubLink" class="control-label"></label>
                <input asp-for="GitHubLink" class="form-control" />
                <span asp-validation-for="GitHubLink" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="YouTubeLink" class="control-label"></label>
                <input asp-for="YouTubeLink" class="form-control" />
                <span asp-validation-for="YouTubeLink" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EvaluationStatus" class="control-label"></label>
                <input asp-for="EvaluationStatus" class="form-control" />
                <span asp-validation-for="EvaluationStatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">LİSTEYE DÖN</a>
</div>

